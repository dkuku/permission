<h1>Sign in</h1>
<%= form_for @conn, session_path(@conn, :create), [as: :session], fn f -> %>
  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>
  <div>
    <%= submit "Sign in" %>
  </div>
<% end %>
